# Some systems don't support "uname -p" and thus require a special case.

if [[ -a /proc/cpuinfo ]]; then
    case `uname -s`===`uname -m` in
	CYGWIN*===*64*)	export CPU=Cygwin_x86_64 ;;
	CYGWIN*)	export CPU=Cygwin_i386 ;;
	Linux*===*64*)	export CPU=Linux_x86_64 ;;
	Linux*)		export CPU=Linux_i386 ;;
    esac
elif [[ -a /bin/hp-pa ]] && /bin/hp-pa; then
    export CPU=`uname -s`_pa
else
    export CPU=`uname -s`_`uname -p`
fi
